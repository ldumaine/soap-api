{
    "name": "zimbra-api/struct",
    "homepage": "https://github.com/zimbra-api",
    "description": "Struct libraries used by Zimbra Api",
    "keywords": ["zimbra", "api", "struct"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": ">=7.2",
        "zimbra-api/common": "self.version",
        "zimbra-api/enum": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "fakerphp/faker": "^1.0"
    },
    "autoload": {
        "psr-4": { "Zimbra\\Struct\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
