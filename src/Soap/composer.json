{
    "name": "zimbra-api/soap",
    "homepage": "https://github.com/zimbra-api",
    "description": "Soap libraries used by Zimbra Api",
    "keywords": ["soap"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": ">=7.2",
        "evenement/evenement": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "zimbra-api/struct": "self.version"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": { "Zimbra\\Soap\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
