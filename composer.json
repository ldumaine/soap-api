{
    "name": "zimbra-api/soap-api",
    "type": "library",
    "description": "PHP wrapper library around the Zimbra Soap API (web service)",
    "keywords": ["library", "soap", "web service", "client", "Zimbra API"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "evenement/evenement": "^3.0",
        "guzzlehttp/guzzle": "^7.2",
        "jms/serializer": "^3.10",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^6.0 || ^7.0 || ^8.0",
        "fzaninotto/faker": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "Zimbra\\": "src/Zimbra"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
